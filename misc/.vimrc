set tabstop=4
set shiftwidth=4
set smarttab
set ai
set cin
set showmatch
set incsearch
set ignorecase
set number
set relativenumber
set noswapfile

colorscheme habamax
syntax on

let mapleader=" "
map - :Ex<CR>
map [c :cnext<CR>
map ]c :cprev<CR>
map <leader>co :copen<CR>
map <leader>cc :cclose<CR>
